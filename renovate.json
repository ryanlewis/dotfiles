{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "branchPrefix": "renovate/",
  "labels": [
    "dependencies",
    "renovate"
  ],
  "commitMessagePrefix": "chore(deps):",
  "commitMessageAction": "update",
  "commitMessageTopic": "{{depName}}",
  "commitMessageExtra": "to {{newVersion}}",
  "dependencyDashboard": true,
  "dependencyDashboardTitle": "ðŸŽ¯ Dependency Dashboard",
  "dependencyDashboardAutoclose": true,
  "prConcurrentLimit": 5,
  "prCreation": "immediate",
  "automerge": false,
  "packageRules": [
    {
      "description": "Automerge patch and minor updates",
      "matchUpdateTypes": [
        "patch",
        "minor"
      ],
      "automerge": true
    }
  ],
  "customManagers": [
    {
      "customType": "regex",
      "description": "Update asdf version in chezmoi script",
      "managerFilePatterns": [
        "/.chezmoiscripts/run_once_01-install-asdf.sh.tmpl/"
      ],
      "matchStrings": [
        "ASDF_VERSION=\"(?<currentValue>\\d+\\.\\d+\\.\\d+)\""
      ],
      "depNameTemplate": "asdf-vm/asdf",
      "datasourceTemplate": "github-releases"
    },
    {
      "customType": "regex",
      "description": "Update FZF version in install.sh",
      "managerFilePatterns": [
        "/^install\\.sh$/"
      ],
      "matchStrings": [
        "FZF_VERSION=\"(?<currentValue>\\d+\\.\\d+\\.\\d+)\""
      ],
      "depNameTemplate": "junegunn/fzf",
      "datasourceTemplate": "github-releases"
    },
    {
      "customType": "regex",
      "description": "Update delta version in install.sh",
      "managerFilePatterns": [
        "/^install\\.sh$/"
      ],
      "matchStrings": [
        "DELTA_VERSION=\"(?<currentValue>\\d+\\.\\d+\\.\\d+)\""
      ],
      "depNameTemplate": "dandavison/delta",
      "datasourceTemplate": "github-releases"
    },
    {
      "customType": "regex",
      "description": "Update atuin version in install.sh",
      "managerFilePatterns": [
        "/^install\\.sh$/"
      ],
      "matchStrings": [
        "ATUIN_VERSION=\"v(?<currentValue>\\d+\\.\\d+\\.\\d+)\""
      ],
      "depNameTemplate": "atuinsh/atuin",
      "datasourceTemplate": "github-releases",
      "versioningTemplate": "semver"
    },
    {
      "customType": "regex",
      "description": "Update just version in install.sh",
      "managerFilePatterns": [
        "/^install\\.sh$/"
      ],
      "matchStrings": [
        "JUST_VERSION=\"(?<currentValue>\\d+\\.\\d+\\.\\d+)\""
      ],
      "depNameTemplate": "casey/just",
      "datasourceTemplate": "github-releases"
    },
    {
      "customType": "regex",
      "description": "Update kubectx version in install.sh",
      "managerFilePatterns": [
        "/^install\\.sh$/"
      ],
      "matchStrings": [
        "KUBECTX_VERSION=\"(?<currentValue>\\d+\\.\\d+\\.\\d+)\""
      ],
      "depNameTemplate": "ahmetb/kubectx",
      "datasourceTemplate": "github-releases"
    },
    {
      "customType": "regex",
      "description": "Update duf version in install.sh",
      "managerFilePatterns": [
        "/^install\\.sh$/"
      ],
      "matchStrings": [
        "DUF_VERSION=\"(?<currentValue>\\d+\\.\\d+\\.\\d+)\""
      ],
      "depNameTemplate": "muesli/duf",
      "datasourceTemplate": "github-releases",
      "versioningTemplate": "semver"
    },
    {
      "customType": "regex",
      "description": "Update dust version in install.sh",
      "managerFilePatterns": [
        "/^install\\.sh$/"
      ],
      "matchStrings": [
        "DUST_VERSION=\"(?<currentValue>\\d+\\.\\d+\\.\\d+)\""
      ],
      "depNameTemplate": "bootandy/dust",
      "datasourceTemplate": "github-releases",
      "versioningTemplate": "semver"
    },
    {
      "customType": "regex",
      "description": "Update tool versions in chezmoi install script",
      "managerFilePatterns": [
        "/.chezmoiscripts/run_onchange_02-install-tools.sh.tmpl/"
      ],
      "matchStrings": [
        "FZF_VERSION=\"(?<currentValue>\\d+\\.\\d+\\.\\d+)\"",
        "BTOP_VERSION=\"(?<currentValue>\\d+\\.\\d+\\.\\d+)\"",
        "DUF_VERSION=\"(?<currentValue>\\d+\\.\\d+\\.\\d+)\"",
        "DUST_VERSION=\"(?<currentValue>\\d+\\.\\d+\\.\\d+)\"",
        "GUM_VERSION=\"(?<currentValue>\\d+\\.\\d+\\.\\d+)\"",
        "JUST_VERSION=\"(?<currentValue>\\d+\\.\\d+\\.\\d+)\""
      ],
      "depNameTemplate": "{{{depName}}}",
      "datasourceTemplate": "github-releases"
    }
  ],
  "asdf": {
    "enabled": true
  },
  "github-actions": {
    "enabled": true,
    "managerFilePatterns": [
      "/.github/workflows/*.yml/",
      "/.github/workflows/*.yaml/"
    ]
  }
}
